{
  "name": "itsolutionssg/yii2-swiper",
  "description": "Yii2 extension for Swiper slider; Php 8.2 ready;",
  "keywords": [
    "yii",
    "yii2",
    "extension",
    "widget",
    "swiper",
    "slider"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sven GÃ¶hring",
      "email": "it.solutions.goehring@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "yiisoft/yii2": "2.*",
    "bower-asset/swiper": ">=11"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5",
    "phpstan/phpstan": "^2.1",
    "friendsofphp/php-cs-fixer": "^3.75"
  },
  "autoload": {
    "psr-4": {
      "ItSolutionsSG\\yii2\\swiper\\": ""
    }
  },
  "scripts": {
    "unit-test": "vendor/bin/phpunit --colors"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
