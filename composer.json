{
  "name": "renschs/yii2-swiper",
  "description": "Yii2 extension for Swiper slider; Php 7.3 ready;",
  "keywords": [
    "yii",
    "yii2",
    "extension",
    "widget",
    "swiper",
    "slider"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sven Rensch",
      "email": "s.rensch87@web.de"
    }
  ],
  "require": {
    "php": ">=7.3",
    "yiisoft/yii2": "2.0.*@dev",
    "bower-asset/swiper": ">=5"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "renschs\\yii2\\swiper\\": ""
    }
  },
  "scripts": {
    "unit-test": "vendor/bin/phpunit --colors"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}